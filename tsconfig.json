{
  "compilerOptions": {
    "lib": ["dom", "ES2020"],
    "strict": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "noEmit": true,
    "moduleResolution": "node"
  },
  "exclude": ["node_modules"]
}
